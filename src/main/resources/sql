create table user (id int not null auto_increment, fullName varchar(225) not null, login varchar(225), email varchar(225), age int,
 subscriptionId int, author boolean, moderator boolean, folowerId int, primary key(id) )

create table post (id int not null auto_increment, create table post (id int auto_increment, title varchar (100), content varchar(1000),
                                                    author varchar(50) not null, moderator varchar(50), rating int,
                                                     status varchar(225), primary key(id))